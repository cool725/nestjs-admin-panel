  "scripts": {
    "ng": "nx",
    "start": "ts-node src/index.ts",
    "build": "nx build api-auth && nx build",
    "api": "nx serve api",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "typeorm:migrate": "npm run typeorm migration:generate -- -n",
    "typeorm:create": "npm run typeorm migration:create --n",
    "typeorm:run": "npm run typeorm migration:run",
    "app:start:app-auth": "nx build app-auth --watch --configuration=development",
    "app:start:app-start": "nx build app-start --watch  --configuration=development",
    "app:start:app-business": "nx build app-business --watch  --configuration=development",
    "api:start:app-business": "nx build app-business --watch --configuration=development",
    "api:start:api-business": "nx build api-business --watch",
    "postinstall": "ngcc --properties es2015 browser module main",
    "docker:build": "docker image build --tag admin-framework  --progress plain --file Dockerfile ."
  },
